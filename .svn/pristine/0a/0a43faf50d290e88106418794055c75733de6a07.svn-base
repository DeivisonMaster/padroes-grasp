package acoplamento.controle;

public class NovaCalculadora {
	private double n1;
	private double n2;
	
	public void setN1(double n1) {
		this.n1 = n1;
	}

	public void setN2(double n2) {
		this.n2 = n2;
	}
	
	/* Cada funcionalidade em seu respectivo método */
	public double soma(){
		return n1 + n2;
	}
	
	public double subtracao(){
		return n1 - n2;
	}
	
	public double multiplicacao(){
		return n1 * n2;
	}
	
	public double divisao(){
		return n1 / n2;
	}
	
}





















